тулованными спортсменами в различных федерациях.\r\n                        Основной целью судьи является, соблюдение правил формата и оказание грамотной, квалифицированной\r\n                        помощи спортсмену, как в разъяснении правил формата так и в обоснованном ответе, для устранения,\r\n                        исправления или улучшения системы спортсмена на будущее. В данном формате в основном борются\r\n                        повседневные системы, которые разделены классами, имеющими свои ограничение как для той или иной\r\n                        системы, так и для различных видов кузова автомобиля.\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Rules;"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAE1B,MAAMC,KAAK,GAAGA,CAAA,KAAM;EAChB,oBACID,OAAA;IAAKE,SAAS,EAAC,2EAA2E;IAAAC,QAAA,gBACtFH,OAAA;MAAKE,SAAS,EAAC,gFAAgF;MAAAC,QAAA,eAC3FH,OAAA;QACIE,SAAS,EAAC,yFAAyF;QAAAC,QAAA,EAAC;MACxG;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAK;IAAC;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OACL,CAAC,eACNP,OAAA;MAAKE,SAAS,EAAC,wEAAwE;MAAAC,QAAA,eACnFH,OAAA;QAAKE,SAAS,EAAC,qHAAqH;QAAAC,QAAA,gBAChIH,OAAA;UAAKE,SAAS,EAAC,uEAAuE;UAAAC,QAAA,EAAC;QACvF;UAAAC,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,eACNP,OAAA;UAAKE,SAAS,EAAC,qGAAqG;UAAAC,QAAA,EAAC;QAWrH;UAAAC,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC;MAAA;QAAAH,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OACL;IAAC;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OACL,CAAC;EAAA;IAAAH,QAAA,EAAAC,YAAA;IAAAC,UAAA;IAAAC,YAAA;EAAA,OACL,CAAC;AAEd,CAAC;AAACC,EAAA,GA5BIP,KAAK;AA8BX,eAAeA,KAAK;AAAC,IAAAO,EAAA;AAAAC,YAAA,CAAAD,EAAA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}